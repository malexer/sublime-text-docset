{
    "name": "Sublime Text",
    "package": "sublime-text",
    "index": "www.sublimetext.com/docs/index.html",
    "selectors": {
        "title": {
            "type": "Guide",
            "regexp": " [â€“-] Sublime Text(?: Documentation)?",
            "replacement": ""
        },
        "section.wide > h2": [
            {
                "type": "Module",
                "requiretext": "Module"
            }
        ],
        ":not(section.wide) > h2": {
            "type": "Section",
            "regexp": "\\s*\\d+$",
            "replacement": ""
        },
        "h2 code": {
            "type": "Class"
        },
        "td.mth": [
            {
                "type": "Method",
                "requiretext": "\\(",
                "regexp": "\\(.*\\)",
                "replacement": ""
            },
            {
                "type": "Property",
                "requiretext": "^[^(]+$"
            }
        ],
        "table.kinds tr td:last-child": "Enum",
        "table.enum tr td:first-child span.key": {
            "type": "Value",
            "regexp": "\"",
            "replacement": ""
        },
        "dl.enum dt > span.key, h3 span.key": {
            "type": "Property",
            "regexp": "\"",
            "replacement": ""
        },
        "dl.enum dt > span.property": "Property",
        "tt[id^=type-]": "Type",
        ".enum > code": "Variable",
        ".enum > *:not(pre) > code": "Variable",
        "table.enum td[id^=variable-] > span.setting": "Variable",
        "span.command": "Command",
        "dl.enum > dt span.setting": {
            "type": "Setting",
            "regexp": "\"",
            "replacement": ""
        },
        "li span.setting": {
            "type": "Setting",
            "matchpath": "docs/file_patterns\\.html"
        },
        "li > span.file, .enum span.file": {
            "type": "File",
            "requiretext": "^[^.]"
        },
        "h4 > code": {
            "type": "Value",
            "regexp": "\\.",
            "replacement": ""
        }
    },
    "ignore": [
        "",
        "(no methods)"
    ],
    "icon32x32": "favicon.png",
    "allowJS": false,
    "ExternalURL": ""
}
